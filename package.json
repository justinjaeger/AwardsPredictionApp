{
  "name": "awardspredictionapp",
  "version": "0.0.2",
  "description": "All-In-One RN Template for Indie Hackers",
  "private": false,
  "scripts": {
    "typescript": "tsc --noEmit --skipLibCheck",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='./index.ios.js' --bundle-output='./ios/awardspredictionapp/main.jsbundle' --assets-dest='./ios/awardspredictionapp' --dev=false --platform='ios'",
    "bundle-android": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='./index.android.js' --bundle-output='./android/app/src/main/assets/main.jsbundle' --assets-dest='./android/app/src/main/res/' --dev=false --platform='android'",
    "clean": "react-native-clean-project"
  },
  "contributors": [
    {
      "name": "Joey Goksu",
      "email": "me@joeygoksu.com"
    }
  ],
  "dependencies": {
    "@eva-design/eva": "^2.1.0",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-native-picker/picker": "^2.4.2",
    "@react-navigation/bottom-tabs": "^5.x",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "@tanstack/react-query": "^4.14.1",
    "@ui-kitten/components": "^5.1.0",
    "@ui-kitten/eva-icons": "^5.1.0",
    "amazon-cognito-identity-js": "^5.2.10",
    "aws-amplify": "^4.3.31",
    "aws-amplify-react-native": "^6.0.5",
    "axios": "^0.27.2",
    "expo-status-bar": "^1.4.2",
    "lodash": "^4.17.21",
    "lottie-react-native": "4.0.2",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-bootsplash": "^3.2.3",
    "react-native-cache": "^2.0.2",
    "react-native-config": "^1.4.2",
    "react-native-draggable-flatlist": "^3.1.2",
