{
  "name": "awardspredictionapp",
  "version": "2.11",
  "description": "Awards Prediction App",
  "private": false,
  "scripts": {
    "typescript": "tsc --noEmit",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone SE (3rd generation)\"",
    "ios:new": "export NODE_OPTIONS=--max-old-space-size=8192 && react-native run-ios --simulator=\"iPhone 14\"",
    "reset-env": "rm -rf node_modules/.cache/babel-loader/* && yarn && react-native start --reset-cache",
    "free-up-memory": "node --max-old-space-size=8192 node_modules/react-native/local-cli/cli.js",
    "start": "react-native start --reset-cache --port=8082",
    "fix": "yarn fix:prettier && yarn fix:eslint",
    "fix:eslint": "eslint --fix \"**/*.ts\"",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "eslint .",
    "lint:old": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"**/*.ts\"",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='./index.js' --bundle-output='./ios/awardspredictionapp/main.jsbundle' --assets-dest='./ios/awardspredictionapp' --dev=false --platform='ios'",
    "bundle-android": "node node_modules/react-native/local-cli/cli.js bundle --entry-file='./index.android.js' --bundle-output='./android/app/src/main/assets/main.jsbundle' --assets-dest='./android/app/src/main/res/' --dev=false --platform='android'",
    "pod-clean-cache": "rm -rf ~/Library/Caches/CocoaPods; rm -rf Pods; rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "pod-clean": "cd ios && pod deintegrate; pod setup; pod install;",
    "postinstall": "patch-package",
    "watchman-clean": "watchman watch-del-all"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@eva-design/eva": "^2.2.0",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/datetimepicker": "^7.1.0",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-native-picker/picker": "^2.4.2",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@shopify/flash-list": "^1.6.3",
    "@tanstack/react-query": "^4.35.7",
    "@ui-kitten/components": "^5.3.1",
    "@ui-kitten/eva-icons": "^5.3.1",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "lottie-react-native": "^5.1.6",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.10",
    "react-native-awesome-slider": "^2.4.6",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-fast-image": "^8.5.12",
    "react-native-gesture-handler": "^2.11.0",
    "react-native-get-random-values": "^1.9.0",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-image-picker": "^5.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.1.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localize": "^2.2.4",
    "react-native-pager-view": "^6.2.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-snackbar": "^2.4.0",
    "react-native-svg": "^13.9.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-swipeable-item": "^2.0.9",
    "react-native-tab-view": "^3.5.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "^11.23.0",
    "styled-components": "^6.0.0-rc.2",
    "title-case": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.184",
    "@types/react": "^18.2.24",
    "@types/react-native": "^0.72.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-plugin-styled-components": "^2.1.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sonarjs": "^0.8.0-125",
    "hex-rgb": "^5.0.0",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.4.9",
    "typescript": "^5.2.2"
  }
}
